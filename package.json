  "devDependencies": {
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "live-server": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "rollup": "^3.25.0",
    "rollup-plugin-obfuscator": "^1.0.0",
    "serve": "^14.2.0"
  }
}
